{
  "name": "ai-agent-sdk-frontend",
  "version": "1.0.0",
  "description": "Frontend dashboard for AI Agent SDK",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
