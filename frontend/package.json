  "packageManager": "pnpm@8.7.0",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "antd": "^5.8.0",
    "@ant-design/icons": "^5.2.0",
    "@ant-design/charts": "^1.4.0",
    "zustand": "^4.4.0",
    "immer": "^10.0.0",
    "axios": "^1.6.0",
    "dayjs": "^1.11.0",
    "lodash": "^4.17.21",
    "styled-components": "^6.0.0",
    "react-beautiful-dnd": "^13.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/lodash": "^4.14.0",
